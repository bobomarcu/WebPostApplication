@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="container">
    <h1>Postal Operations</h1>
    <hr />
    <div class="text-lg-start">
        <h3>Personnel</h3>
        <div class="text-center">
            <div class="d-flex flex-row justify-content-start align-items-center">
                <div class="card w-25 m-2 h-50  bg-white border-bottom box-shadow mb-3">
                    <div class="card-body">
                        <img src="data:image/png;base64,">
                        <h2>Couriers</h2>
                        <a asp-page="/Couriers/Index"><button class="btn btn-primary">View</button></a>
                    </div>
                </div>
                <div class="card w-25 m-2 h-50 bg-white border-bottom box-shadow mb-3">
                    <div class="card-body">
                        <img src="data:image/png;base64,">
                        <h2>Users</h2>
                        <a asp-page="/Users/Index"><button class="btn btn-primary">View</button></a>
                    </div>
                </div>
                <div class="card w-25 m-2 h-50 bg-white border-bottom box-shadow mb-3">
                    <div class="card-body">
                        <img src="data:image/png;base64,">
                        <h2>Cashiers</h2>
                        <a asp-page="/Cashiers/Index"><button class="btn btn-primary">View</button></a>
                    </div>
                </div>
                
            </div>

        </div>
    </div>
    <hr />
    <div class="text-lg-start">
        <h3>Packages and Post Offices</h3>
        <div class="text-center">
            <div class="d-flex flex-row justify-content-start align-items-center">
                <div class="card w-25 m-2 h-50 bg-white border-bottom box-shadow mb-3">
                    <div class="card-body">
                        <img src="data:image/png;base64,">
                        <h2>Packages</h2>
                        <a asp-page="/Packages/Index"><button class="btn btn-primary">View</button></a>
                    </div>
                </div>
                <div class="card w-25 m-2 h-50 bg-white border-bottom box-shadow mb-3">
                    <div class="card-body">
                        <img src="data:image/png;base64,">
                        <h2>Post Offices</h2>
                        <a asp-page="/PostOffices/Index"><button class="btn btn-primary">View</button></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


